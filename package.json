{
  "name": "vanilla-bank",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npx webpack --watch\"",
    "dev:server": "nodemon --inspect server/bin/www",
    "server": "node server/bin/www",
    "test": "jest",
    "build": "npx webpack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.6.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "husky": "^4.3.8",
    "jsonwebtoken": "^8.5.1",
    "mobx": "^6.1.5",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sass": "^1.27.0",
    "sequelize": "^6.3.5",
    "session-file-store": "^1.4.0",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "better-module-alias": "^1.1.0",
    "css-loader": "^4.3.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^4.4.1",
    "http-proxy-middleware": "^1.0.5",
    "jest": "^26.5.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "2.1.2",
    "sass-loader": "^10.0.3",
    "style-loader": "^1.2.1",
    "webpack-cli": "^3.3.12"
  },
  "_moduleAliases": {
    "@config": "./server/config",
    "@service": "./server/service",
    "@util": "./server/util",
    "@model": "./server/model",
    "@middleware": "./server/middleware"
  }
}
